# CloudWatch Configuration
# Copy this file to .env and fill in your actual values

# AWS Credentials for CloudWatch logging
VUE_APP_AWS_ACCESS_KEY_ID=your_aws_access_key_here
VUE_APP_AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
VUE_APP_AWS_REGION=us-east-1

# CloudWatch Log Configuration
VUE_APP_LOG_GROUP_NAME=acm-website-logs
VUE_APP_LOG_STREAM_NAME=error-stream
VUE_APP_ACTIVITY_STREAM_NAME=activity-stream

# Firebase Configuration (if needed)
VUE_APP_FIREBASE_API_KEY=your_firebase_api_key
VUE_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VUE_APP_FIREBASE_PROJECT_ID=your_project_id
VUE_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VUE_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
VUE_APP_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Development/Production Environment
NODE_ENV=development