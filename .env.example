# CloudWatch Configuration
VUE_APP_AWS_REGION=us-east-1
VUE_APP_AWS_ACCESS_KEY_ID=your_access_key_here
VUE_APP_AWS_SECRET_ACCESS_KEY=your_secret_key_here
VUE_APP_LOG_GROUP_NAME=acm-website-logs
VUE_APP_LOG_STREAM_NAME=error-stream
VUE_APP_ACTIVITY_STREAM_NAME=activity-stream

# Payment Service Configuration
VUE_APP_PAYMENT_API_URL=https://api.payment-service.com
VUE_APP_PAYMENT_API_KEY=your_payment_api_key_here
VUE_APP_PAYMENT_TIMEOUT=5000
VUE_APP_PAYMENT_MAX_RETRIES=3
VUE_APP_PAYMENT_RETRY_DELAY=1000

# Database Configuration
VUE_APP_DB_HOST=localhost
VUE_APP_DB_PORT=5432
VUE_APP_DB_NAME=app_db
VUE_APP_DB_USER=app_user
VUE_APP_DB_PASSWORD=your_db_password_here
VUE_APP_DB_SSL=false
VUE_APP_DB_TIMEOUT=3000
VUE_APP_DB_MAX_RETRIES=3
VUE_APP_DB_RETRY_DELAY=1000

# Connection Pool Configuration
VUE_APP_PAYMENT_POOL_MAX_CONNECTIONS=5
VUE_APP_DATABASE_POOL_MAX_CONNECTIONS=10