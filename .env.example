# Vue.js Environment Variables
# Copy this file to .env and fill in your values

# CloudWatch Configuration
VUE_APP_ENABLE_CLOUDWATCH=true
VUE_APP_AWS_REGION=us-east-1
VUE_APP_LOG_GROUP_NAME=acm-website-logs
VUE_APP_LOG_STREAM_NAME=error-stream
VUE_APP_ACTIVITY_STREAM_NAME=activity-stream

# Firebase Functions Environment Variables (for server-side AWS credentials)
# These should be set in Firebase Functions environment, not in frontend
# AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your_access_key_here
# AWS_SECRET_ACCESS_KEY=your_secret_key_here
# LOG_GROUP_NAME=acm-website-logs
# LOG_STREAM_NAME=error-stream

# Development Settings
NODE_ENV=development