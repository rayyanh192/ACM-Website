# Vue.js Application Environment Variables

# CloudWatch Configuration
VUE_APP_AWS_REGION=us-east-1
VUE_APP_AWS_ACCESS_KEY_ID=your_access_key_here
VUE_APP_AWS_SECRET_ACCESS_KEY=your_secret_key_here
VUE_APP_LOG_GROUP_NAME=acm-website-logs
VUE_APP_LOG_STREAM_NAME=error-stream
VUE_APP_ACTIVITY_STREAM_NAME=activity-stream

# Firebase Configuration
VUE_APP_FIREBASE_API_KEY=your_firebase_api_key
VUE_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VUE_APP_FIREBASE_PROJECT_ID=your_project_id
VUE_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VUE_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
VUE_APP_FIREBASE_APP_ID=your_app_id

# Database Configuration (for Firebase Functions)
DB_HOST=localhost
DB_PORT=3306
DB_NAME=acm_website
DB_USER=root
DB_PASSWORD=your_db_password
DB_CONNECTION_LIMIT=10
DB_ACQUIRE_TIMEOUT=60000
DB_TIMEOUT=60000
DB_RECONNECT=true
DB_QUEUE_LIMIT=0
DB_IDLE_TIMEOUT=300000
DB_MAX_RETRIES=3
DB_RETRY_DELAY=2000
DB_SSL_REJECT_UNAUTHORIZED=true
DB_HEALTH_CHECK_INTERVAL=30000
DB_HEALTH_CHECK_TIMEOUT=5000
DB_DEBUG=false
DB_LOG_QUERIES=false
DB_LOG_ERRORS=true

# Payment Service Configuration
PAYMENT_SERVICE_URL=https://api.payment-service.com/charge
PAYMENT_SERVICE_HEALTH_URL=https://api.payment-service.com/health
PAYMENT_API_KEY=your_payment_api_key

# SSL Configuration (optional)
DB_SSL_CA=
DB_SSL_CERT=
DB_SSL_KEY=